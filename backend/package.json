{
  "name": "indulink-backend",
  "version": "1.0.0",
  "description": "Indulink E-commerce Backend API - Node.js, Express, MongoDB",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage --runInBand",
    "test:watch": "jest --watchAll",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:coverage": "jest --coverage --coverageReporters=html --coverageReporters=text",
    "validate-monitoring": "node scripts/validate-monitoring.js"
  },
  "keywords": [
    "ecommerce",
    "api",
    "express",
    "mongodb"
  ],
  "author": "Indulink Team",
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "firebase-admin": "^13.6.0",
    "helmet": "^7.1.0",
    "jest-html-reporter": "^3.10.2",
    "jest-junit": "^16.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb-memory-server": "^9.5.0",
    "mongoose": "^9.0.0",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "newrelic": "^13.6.6",
    "nodemailer": "^6.9.7",
    "nodemon": "^3.0.2",
    "nyc": "^15.1.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
